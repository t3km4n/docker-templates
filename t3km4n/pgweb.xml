<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
  <Category>Tools:</Category>
  <Name>t3km4n-pgweb</Name>
  <Description></Description>
  <Registry>https://hub.docker.com/r/t3km4n/deb-pgweb/</Registry>
  <Repository>t3km4n/deb-pgweb:latest</Repository>
  <Environment>
    <Variable>
      <Name>detach</Name>
      <Value>true</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8081</HostPort>
        <ContainerPort>8081</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Version></Version>
   <WebUI>http://[IP]:[PORT:8081]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/t3km4n/docker-templates/main/t3km4n/pgweb.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/t3km4n/docker-templates/main/t3km4n/icons/pgweb.png</Icon>
</Container>
